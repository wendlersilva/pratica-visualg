algoritmo "Exercício Função Par ou Impar"
// Função :
// Autor :
// Data : 30/07/2023
// Seção de Declarações 
var
    numero:inteiro

inicio

    Escreval("Digite um número")
    Leia(numero)
    
    Se( numero mod 2 = 0) Entao
        Escreval("Ele é par")
    
    Senao
        Escreval("Ele é impar")
    
    FimSe


fimalgoritmo

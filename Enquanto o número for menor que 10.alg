algoritmo "Enquanto número for diferente "
// Função :
// Autor :
// Data : 01/08/2023
// Seção de Declarações 
var
        numero:inteiro

inicio

        numero <- 1
        
        Enquanto numero < 10 Faca
             Escreval(numero)
             numero <- numero + 1
        
        FimEnquanto

fimalgoritmo

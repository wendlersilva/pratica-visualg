algoritmo "semnome"
// Função :
// Autor :
// Data : 28/07/2023
// Seção de Declarações 
var

    minutos, totalPagar: real

inicio

    escreval("Digite a quantidade de minutos utilizada pelo cliente")
    leia(minutos)
    
    totalPagar <- minutos * ((1/60)*2)
    
    escreval("Total a pagar é de:" ,totalPagar, " reais")

fimalgoritmo

algoritmo "Exercício repita numero"
// Função :
// Autor :
// Data : 05/08/2023
// Seção de Declarações 
var
      numero:inteiro
inicio

      Repita
      Escreval("Digite o número 1")
      Leia(numero)
      
      Ate numero = 1
      
      Escreval("Muito bem, Você digitou o número 1")

fimalgoritmo
